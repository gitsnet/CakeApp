
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title-CakeApp</title>   
    @*@Styles.Render("~/Content/main/css")*@
    <link href="~/Content/site.css" rel="stylesheet" />
    <link href="~/Content/admin/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/admin/css/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/admin/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/admin/select2/select2.css" rel="stylesheet" />
    <link href="~/Content/admin/datatables/plugins/bootstrap/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/admin/css/fileinput.css" rel="stylesheet" />
    
    @Scripts.Render("~/bundles/jquery")
    
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href='@Url.Action("Index","Dashboard")'><img src="~/Content/admin/images/logo.png" alt="" class="img-responsive" onclick="window.location.href = '@Url.Action("Index", "DashBoard")'" /></a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-cube"></i>Product<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li id="liProductLi"><a href='@Url.Action("ProductList", "Products")'>Product Listing</a></li>
                            <li><a href='@Url.Action("ProductImage","Products")'>Product image upload</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-files-o"></i>Category<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href='@Url.Action("CategoryList", "Categories")'>Category Listing</a></li>
                            <li><a href='@Url.Action("CategoryImageList", "CategoryImage")'>Category Image List</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-sitemap"></i>Attribute<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href='@Url.Action("List", "Tag")'>Tag</a></li>
                            <li><a href='@Url.Action("List", "Group")'>Cake Groups</a></li>
                            <li><a href='@Url.Action("List", "Info")'>Info</a></li>
                            <li><a href='@Url.Action("List", "Size")'>Size</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-refresh"></i>Sync<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href='@Url.Action("SyncProcess", "SyncProcess")'>Sync Process</a></li>
                            <li><a href='@Url.Action("SyncEmail", "SyncProcess")'>Sync Email</a></li>
                            <li><a href='@Url.Action("SyncFtp", "SyncProcess")'>Sync Ftp</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-cog fa-fw"></i>Screensaver<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href='@Url.Action("ScreensaverList", "Screensavers")'>Screensaver List</a></li>

                        </ul>
                    </li>
                </ul>

                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/Content/admin/images/avatar3_small.jpg" alt="" />@User.Identity.Name
                        <span class="caret"></span></a>
                        <ul class="dropdown-menu right-dropdown">
                          <li><a href="javascript:void(0);">Settings</a></li>
                          <li><a href='@Url.Action("Logout", "Login")'>Logout</a></li>
                        
                        </ul>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>

    <section class="breadcumb">
        <div class="container">
            <ul>
                <li><a class="fa fa-home" id="home" onclick="window.location.href = '@Url.Action("Index", "DashBoard")'"></a></li>
               
                <li><a href="@ViewBag.href">@ViewBag.Description</a></li>
                <li>@ViewBag.SubDescription</li>
            </ul>
        </div>
    </section>

    
    <div id="mainBody">
        @RenderBody()
    </div>
    @Scripts.Render("~/Content/main/js")
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/bootstrap-validator/0.4.5/css/bootstrapvalidator.min.css">
    
    <script src="/Scripts/jquery.validate.js"></script>
    <script src="/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="/Scripts/jquery.validate.unobtrusive.bootstrap.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
<script>
   

</script>
